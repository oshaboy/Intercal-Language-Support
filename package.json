{
    "name": "intercal-language-support",
    "displayName": "Intercal Language Support",
    "description": "Language Support for the 1972 Esoteric Programming Language.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "intercal",
            "aliases": ["Intercal", "intercal"],
            "extensions": [".i"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "intercal",
            "scopeName": "source.i",
            "path": "./syntaxes/intercal.tmLanguage.json"
        }]
    }
}